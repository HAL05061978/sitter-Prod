{
  "name": "sitter-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:prod": "set NODE_ENV=production && next dev",
    "build": "next build",
    "build:prod": "next build",
    "build:mobile": "set BUILD_MODE=capacitor && next build && npx cap sync",
    "start": "next start",
    "start:prod": "next start",
    "prod": "scripts\\prod-deploy.bat",
    "prod:ps": "powershell -ExecutionPolicy Bypass -File scripts\\prod-deploy.ps1",
    "prod:bash": "bash scripts/prod-deploy.sh",
    "prod:quick": "scripts\\quick-deploy.bat",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "setup": "setup.bat",
    "setup:env": "scripts\\setup-environments.bat",
    "setup:env:new": "scripts\\setup-env.bat",
    "deploy:prod": "scripts\\deploy-production.bat",
    "deploy:functions": "supabase functions deploy --project-ref $SUPABASE_PROJECT_REF",
    "deploy:db": "supabase db push --project-ref $SUPABASE_PROJECT_REF",
    "db:sync": "chmod +x scripts/sync-database.sh && ./scripts/sync-database.sh",
    "setup:db": "powershell -ExecutionPolicy Bypass -File run-production-setup.ps1",
    "cap:add:ios": "npx cap add ios",
    "cap:add:android": "npx cap add android",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "cap:sync": "npx cap sync",
    "cap:run:ios": "npm run build:mobile && npx cap run ios",
    "cap:run:android": "npm run build:mobile && npx cap run android",
    "test": "echo 'No tests configured yet'",
    "test:notifications": "echo 'Testing notification system...' && npm run dev",
    "clean": "if exist .next rmdir /s /q .next && if exist node_modules\\.cache rmdir /s /q node_modules\\.cache",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/background-runner": "^2.2.0",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/geolocation": "^7.1.5",
    "@capacitor/ios": "^7.4.4",
    "@capacitor/local-notifications": "^7.0.3",
    "@capacitor/push-notifications": "^7.0.3",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "latest",
    "@supabase/supabase-js": "latest",
    "@types/leaflet": "^1.9.21",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.17",
    "date-fns": "^2.30.0",
    "i18next": "^25.6.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "leaflet": "^1.9.4",
    "next": "^14.2.33",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "react-easy-crop": "^5.5.3",
    "react-i18next": "^16.3.5",
    "react-leaflet": "^4.2.1",
    "react-swipeable": "^7.0.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "encoding": "^0.1.13",
    "eslint": "^8",
    "eslint-config-next": "^14.0.0",
    "supabase": "^2.34.3"
  }
}
